    {
      "id": "customTool_0",
      "position": {
        "x": 250,
        "y": 500
      },
      "type": "customNode",
      "data": {
        "id": "customTool_0",
        "label": "Code Executor",
        "name": "customTool",
        "type": "CustomTool",
        "baseClasses": ["CustomTool", "Tool", "StructuredTool"],
        "category": "Tools",
        "inputs": {
          "name": "code_executor",
          "description": "Execute Python code and return the results. Input should be valid Python code.",
          "func": "const { exec } = require('child_process');\nconst { promisify } = require('util');\nconst execAsync = promisify(exec);\n\ntry {\n    const code = $input;\n    const wrappedCode = `python -c \"${code.replace(/\"/g, '\\\\\"')}\"`;\n    const { stdout, stderr } = await execAsync(wrappedCode);\n    return stdout || stderr || 'Code executed successfully';\n} catch (error) {\n    return `Error: ${error.message}`;\n}"
        }
      }
    },
